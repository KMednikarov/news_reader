{
  "scraper_name":"FinancialTimes",
  "base_url": "https://ft.com",
  "search_url": "/search?&q={}&dateFrom={}&contentType=article&page={}",
  "data_consent_window": "//*[contains(@id, 'sp_message_iframe')]",
  "data_consent_switch_to_iframe": "True",
  "data_consent_accept_btn": "//button[contains(text(),'Accept Cookies')]",
  "article_main": ".//div[contains(@class,'o-teaser__content')]",
  "article_header": ".//div[contains(@class,'o-teaser__heading')]",
  "article_date": ".//time[contains(@class,'o-teaser__timestamp-date')]",
  "article_date_format": "%Y-%m-%dT%H:%M:%S%z",
  "pages_text": ".//span[contains(@class,'search-pagination__page')]"
}

